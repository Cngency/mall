<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="com.scoprion.mall.wx.mapper.WxTicketMapper">

    <!-- 优惠卷列表 -->
    <select id="findByTicketList" resultType="com.scoprion.mall.domain.Ticket">
        SELECT
        userId,
        number,
        ticketId,
        ticketName,
        content
        FROM t_ticket_user t_t_u LEFT JOIN t_ticket t_t ON t_t_u.ticketId=t_t.id
    </select>


</mapper>