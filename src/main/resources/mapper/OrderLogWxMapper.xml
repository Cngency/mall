<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.scoprion.mall.littlesoft.mapper.OrderLogWxMapper">
    <insert id="add" parameterType="com.scoprion.mall.domain.OrderExt">
        INSERT INTO t_wx_mall_order_log(goodNo,
        sellerNo,
        type,
        ipAddress,
        createDate,
        description,)
        VALUES (#{goodNo},
        #{sellerNo},
        #{type},
        NOW(),
        #{description}
        )
    </insert>
</mapper>